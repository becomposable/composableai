{
  "name": "@becomposable/api-fetch-client",
  "version": "0.31.0",
  "description": "HTTP client which wraps a fetch implementation and simplify the creation of REST API clients. Works both in browser and in node.",
  "type": "module",
  "types": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "test": "ESBK_TSCONFIG_PATH=./test/tsconfig.json npx mocha -r tsx --exit ./test/**/*.ts",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && ./build.sh",
    "dist": "npm run build && rm ./lib/*tsbuildinfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bstefanescu/api-fetch-client.git"
  },
  "keywords": [
    "fetch",
    "client",
    "api",
    "rest",
    "browser",
    "node",
    "sse",
    "eventsource"
  ],
  "author": "stefanescu.bogdan@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bstefanescu/api-fetch-client/issues"
  },
  "homepage": "https://github.com/bstefanescu/api-fetch-client#readme",
  "devDependencies": {
    "@koa-stack/router": "^0.9.0",
    "@koa-stack/server": "^0.9.0",
    "@types/koa": "^2.13.8",
    "@types/mocha": "^10.0.1",
    "koa": "^2.14.2",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "eventsource-parser": "^1.1.1"
  }
}