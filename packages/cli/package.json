{
    "name": "@vertesia/cli",
    "version": "0.43.0",
    "description": "Code generation for the interaction defined with Composable AI Studio",
    "type": "module",
    "bin": {
        "composable": "./bin/app.js"
    },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "files": [
        "templates",
        "lib",
        "bin"
    ],
    "license": "MIT",
    "homepage": "https://composableprompts.com/docs",
    "keywords": [
        "llm",
        "code",
        "generation",
        "interaction",
        "composable",
        "prompt",
        "ai"
    ],
    "scripts": {
        "eslint": "eslint './src/**/*.{jsx,js,tsx,ts}'",
        "build": "rm -rf ./lib ./tsconfig.tsbuildinfo && tsc --build",
        "clean": "rimraf ./node_modules ./lib ./tsconfig.tsbuildinfo"
    },
    "dependencies": {
        "@vertesia/client": "workspace:*",
        "@vertesia/common": "workspace:*",
        "@vertesia/memory-cli": "^0.43.0",
        "@vertesia/memory-commands": "^0.43.0",
        "@vertesia/workflow": "workspace:*",
        "ansi-colors": "^4.1.3",
        "ansi-escapes": "^6.2.0",
        "cli-spinners": "^2.9.1",
        "commander": "^12.1.0",
        "enquirer": "^2.4.1",
        "glob": "^11.0.0",
        "json-schema": "^0.4.0",
        "json-schema-to-typescript": "^13.1.1",
        "jsonwebtoken": "^9.0.2",
        "mime": "^4.0.4",
        "node-web-stream-adapters": "^0.2.1",
        "open": "^10.1.0",
        "seedrandom": "^3.0.5",
        "signal-exit": "^4.1.0",
        "typescript": "^5.5.3"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^9.0.6"
    }
}