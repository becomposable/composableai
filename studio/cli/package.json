{
    "name": "@composableai/cli",
    "version": "0.26.0",
    "description": "Code generation for the interaction defined with Composable AI Studio",
    "type": "module",
    "bin": {
        "cpcli": "./bin/app.js"
    },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "files": [
        "templates",
        "lib",
        "bin"
    ],
    "license": "MIT",
    "homepage": "https://composableprompts.com/docs",
    "keywords": [
        "llm",
        "code",
        "generation",
        "interaction",
        "composable",
