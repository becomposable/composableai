        "ansi-colors": "^4.1.3",
        "ansi-escapes": "^6.2.0",
        "api-fetch-client": "0.12.0",
        "cli-spinners": "^2.9.1",
        "commander": "^11.1.0",
        "enquirer": "^2.4.1",
        "glob": "^11.0.0",
        "json-schema": "^0.4.0",
        "json-schema-to-typescript": "^13.1.1",
        "node-web-stream-adapters": "^0.1.0",
        "open": "^10.1.0",
