        "clean": "rimraf ./node_modules ./lib ./tsconfig.tsbuildinfo"
    },
    "dependencies": {
        "@composableai/studio-client": "^0.26.0",
        "@composableai/studio-common": "^0.26.0",
        "@composableai/workflow": "^0.26.0",
